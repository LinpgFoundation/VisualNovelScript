{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Visual Novel Script",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
                "name": "keyword.control.vns",
                "match": "\\[(.*?)\\]"
            }]
		},
		"strings": {
			"name": "string.unquoted.vns",
			"begin":"- ",
			"end": "\n"
		}
	},
	"scopeName": "source.vns"
}